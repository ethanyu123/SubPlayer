{
  "name": "subplayer",
  "version": "2.0.0",
  "description": "SubPlayer is a online subtitle editor",
  "homepage": "https://subplayer.js.org",
  "main": "main.js",
  "dependencies": {
    "artplayer-react": "^3.5.13",
    "duration-time-conversion": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "option-validator": "^2.0.6",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-i18nify": "^3.1.7",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.0.8",
    "react-virtualized": "^9.22.2",
    "reactcss": "^1.2.3",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-app-rewired build && rm -rf docs && mv build docs",
    "build:electron": "npm run build && electron .",
    "lint": "npx eslint src",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "customize-cra": "^1.0.0",
    "electron": "^11.0.3",
    "react-app-rewired": "^2.1.6"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "subplayer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}